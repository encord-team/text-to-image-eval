{
  "$defs": {
    "Split": {
      "enum": [
        "train",
        "validation",
        "test",
        "all"
      ],
      "title": "Split",
      "type": "string"
    }
  },
  "additionalProperties": true,
  "properties": {
    "dataset_type": {
      "title": "Dataset Type",
      "type": "string"
    },
    "module_path": {
      "format": "path",
      "title": "Module Path",
      "type": "string"
    },
    "title": {
      "title": "Title",
      "type": "string"
    },
    "split": {
      "allOf": [
        {
          "$ref": "#/$defs/Split"
        }
      ],
      "default": "all"
    },
    "title_in_source": {
      "anyOf": [
        {
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Title In Source"
    },
    "cache_dir": {
      "anyOf": [
        {
          "format": "path",
          "type": "string"
        },
        {
          "type": "null"
        }
      ],
      "default": null,
      "title": "Cache Dir"
    }
  },
  "required": [
    "dataset_type",
    "module_path",
    "title"
  ],
  "title": "DatasetDefinitionSpec",
  "type": "object"
}
